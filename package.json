{
  "name": "SCMobile",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-react-native",
    "setupFiles": [
      "./test-setup.js"
    ],
    "preprocessorIgnorePatterns": [
      "node_modules/(?!react-native|react-clone-referenced-element|tcomb-form-native|spatialconnect-form-schema|react-native-slider|react-native-button)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ios.js"
    ],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[./a-zA-Z0-9$_-]+\\.png$": "RelativeImageStub"
    }
  },
  "dependencies": {
    "jwt-decode": "2.0.1",
    "react": "15.3.0-rc.3",
    "react-native": "0.30.0",
    "react-native-button": "^1.6.0",
    "react-native-drawer": "^2.2.3",
    "react-native-image-picker": "0.21.2",
    "react-native-maps": "file:../react-native-maps",
    "react-native-router-flux": "3.31.0",
    "react-native-slider": "^0.8.0",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "rx": "^4.1.0",
    "slash": "^1.0.0",
    "spatialconnect": "file:../spatialconnect-js",
    "spatialconnect-form-schema": "file:../spatialconnect-form-schema",
    "tcomb-form-native": "^0.5.1",
    "tcomb-json-schema": "^0.3.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-jest": "14.1.0",
    "babel-plugin-rewire": "^1.0.0-rc-3",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-react-native": "1.9.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.3.0",
    "jest": "14.1.0",
    "jest-react-native": "14.1.2",
    "jsdom": "^8.4.1",
    "mocha": "^2.4.5",
    "mock-browser": "^0.92.11",
    "mockery": "^1.6.2",
    "react-addons-test-utils": "^0.14.6",
    "react-native-mock": "^0.2.3",
    "react-test-renderer": "15.3.0",
    "redux-mock-store": "1.1.2",
    "should": "9.0.2",
    "sinon": "^1.17.3"
  }
}
